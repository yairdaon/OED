@Article{DettoriGottliebTemam95,
  Title                    = {A nonlinear {G}alerkin method: the two-level {F}ourier-collocation case},
  Author                   = {Dettori, L. and Gottlieb, D. and Temam, R.},
  Journal                  = {Journal of Computational Physics},
  Year                     = {1995},
  Number                   = {4},
  Pages                    = {371--89},
  Volume                   = {10},

  Abstract                 = {We study the implementation of the nonlinear Galerkin method as a multiresolution method when a two-level Fourier-collocation discretization is used. The set of collocation points with an even number of points is considered as the fine grid and decomposed into two coarse grids containing half as many points. Using these two grids we decompose the unknown into the sum of a large scale component containing only low frequency modes and based on one of the coarse grids and a small scale component containing only high frequency modes and based on the other coarse grid. This produces interesting connections between the physical space and the Fourier space representations of the function. A nonlinear Galerkin scheme is then applied to the Burgers equation; finally, implementation issues are discussed showing the advantages of the method.},
  Owner                    = {ccgo},
  Timestamp                = {2007.04.25}
}

