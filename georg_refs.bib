@Article{BursteddeStadlerAlisicEtAl13,
  Title                    = {Large-scale adaptive mantle convection simulation},
  Author                   = {Burstedde, Carsten and Stadler, Georg and Alisic, Laura and Wilcox, Lucas C. and Tan, Eh and Gurnis, Michael and Ghattas, Omar},
  Journal                  = {Geophysical Journal International},
  Year                     = {2013},
  Number                   = {3},
  Pages                    = {889-906},
  Volume                   = {192},

  Abstract                 = {A new generation, parallel adaptive-mesh mantle convection code, Rhea, is described and benchmarked. Rhea targets large-scale mantle convection simulations on parallel computers, and thus has been developed with a strong focus on computational efficiency and parallel scalability of both mesh handling and numerical solvers. Rhea builds mantle convection solvers on a collection of parallel octree-based adaptive finite element libraries that support new distributed data structures and parallel algorithms for dynamic coarsening, refinement, rebalancing and repartitioning of the mesh. In this study we demonstrate scalability to 122 880 compute cores and verify correctness of the implementation. We present the numerical approximation and convergence properties using 3-D benchmark problems and other tests for variable-viscosity Stokes flow and thermal convection.},
  Doi                      = {10.1093/gji/ggs070}
}

@Article{BursteddeWilcoxGhattas11,
  Title                    = {{\texttt{p4est}}: {S}calable Algorithms for Parallel Adaptive Mesh Refinement on Forests of Octrees},
  Author                   = {Carsten Burstedde and Lucas C. Wilcox and Omar Ghattas},
  Journal                  = {SIAM Journal on Scientific Computing},
  Year                     = {2011},
  Number                   = {3},
  Pages                    = {1103-1133},
  Volume                   = {33},

  Ajournal                 = {SIAM J. Sci. Comput.},
  Doi                      = {10.1137/100791634},
  File                     = {BursteddeWilcoxGhattas11.pdf:BursteddeWilcoxGhattas11.pdf:PDF},
  ISSN                     = {1064-8275},
  Mrclass                  = {65N50 (65D17 65Y05 68W10)},
  Mrnumber                 = {2800566 (2012f:65209)},
  Mrreviewer               = {Kenneth Wright},
  Owner                    = {ccgo},
  Review                   = {This paper is concerned with adaptive mesh refinement in two and three dimensions using quadtrees and octrees respectively. The software p4est, which is described in the paper, is designed for large-scale parallel processing and to be scalable. Rather than using a single tree a forest of trees is used so that a wide variety of geometries can be dealt with. The software implements the creation and manipulation of these structures. It is designed to interface with external software such as those for the numerical solution of partial differential equations. Consequently the criteria for refinement are provided externally. Full details of how the quadtree and octree forests are labelled and manipulated are given. This includes refinement and coarsening of the structure, and the possibility of including the requirement that ratios of adjacent element sizes are not greater than 2. The distribution and redistribution of data over a possibly large number of processes is also dealt with in detail. Synthetic and practical geophysical examples of the use of the software are given, and these illustrate the scalability of the software.},
  Timestamp                = {2010.01.15}
}

